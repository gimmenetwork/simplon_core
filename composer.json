{
  "name": "gimmenetwork/simplon_core",
  "description": "[FORK] Strongly opinionated MVC framework for rapid development",
  "homepage": "https://github.com/fightbulc/simplon_core",
  "type": "library",
  "keywords": [
    "mvc",
    "model",
    "view",
    "controller",
    "templates",
    "router",
    "mustache",
    "locales",
    "i18n",
    "fast",
    "lightweight",
    "form",
    "doi"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tino Ehrich",
      "email": "tino@bigpun.me",
      "role": "developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_locale"
    },{
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_form"
    },{
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_url"
    },{
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_template"
    },{
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_device"
    },{
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_helper"
    },{
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_interfaces"
    },{
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_http"
    }
  ],
  "require": {
    "php": ">=7.2",
    "gimmenetwork/simplon_locale": "^0.4",
    "gimmenetwork/simplon_form": "^2.5",
    "gimmenetwork/simplon_url": "^0.6",
    "gimmenetwork/simplon_template": "^0.8",
    "gimmenetwork/simplon_device": "^0.3",
    "gimmenetwork/simplon_helper": "^1.0",
    "gimmenetwork/simplon_interfaces": "^0.0",
    "gimmenetwork/simplon_http": "^0.0",
    "relay/relay": "^1.1",
    "filp/whoops": "^2.1",
    "mnapoli/silly": "1.6.*",
    "spatie/array-to-xml": "^2.7",
    "zendframework/zend-diactoros": "^2.2"
  },
  "config": {
    "component-dir": "public/assets/vendor"
  },
  "autoload": {
    "psr-4": {
      "Simplon\\Core\\": "src/"
    }
  },
  "bin": [
    "bin/core"
  ],
  "replace": {
    "simplon/core": "0.9.*"
  }
}
